.head-block__main
  header
    .content-center
      .container-fluid
        .row
          .col-sm-3.head_block__logo
            img src='<%=asset_path "assets/img/logo.png"%>'
          .col-sm-6.head_block__search
            input type="search" placeholder="enter a keyword to search"
          .col-sm-3.head_block__cart
            shopping-cart

  nav
    .content-center
      ul
        li ng-repeat="navlabel in navLabels"
          a{{navlabel.label}}
      .clearfix
main
  .carousel-block
    carousel-wrapper slides="slides" carousel-interval="carouselInterval"
.main-product__tabs
  .content-center
    h1 product tabs
    ul
      li
        a href="#" FEATURED PRODUCTS
      li
        '/
      li
        a href="#" NEW PRODUCTS
      li
        '/
      li
        a href="#" RANDOM PRODUCTS
    .main-product__content
